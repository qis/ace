// config.h.in based on src/windows/vs2019/config.h
//
// grep -E '.*#\s*(cmakedefine|define|undef)\s+' config.h.in | \
//  sed -E 's/.*#\s*(cmakedefine|define|undef)\s+//g' | \
//  sed -E 's/\s+.*//g' | xargs -iNaMe \
//  find src -name '*.[ch]' -not -name config.h -exec \
//  grep --color -HF 'NaMe' '{}' ';'
//
// Removed:
// - APIs not present on Linux or Windows.
// - Third party hash functions.
// - Third party libraries not available in sysroot.
// - Localization.
//
