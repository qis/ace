{
  "version": 6,
  "cmakeMinimumRequired": {
    "major": 4,
    "minor": 1
  },
  "configurePresets": [
    {
      "hidden": true,
      "name": "default",
      "generator": "Ninja",
      "binaryDir": "${sourceDir}/build/${presetName}",
      "installDir": "${sourceDir}/install/${presetName}",
      "architecture": { "value": "x64", "strategy": "external" },
      "warnings": { "dev": false },
      "cacheVariables": {
        "CMAKE_C_STANDARD": "23",
        "CMAKE_C_STANDARD_REQUIRED": true,
        "CMAKE_CXX_STANDARD": "26",
        "CMAKE_CXX_STANDARD_REQUIRED": true,
        "CMAKE_CXX_SCAN_FOR_MODULES": true
      }
    },
    {
      "hidden": true,
      "name": "linux",
      "toolchainFile": "$env{ACE}/linux.cmake",
      "condition": {
        "type": "equals",
        "lhs": "${hostSystemName}",
        "rhs": "Linux"
      }
    },
    {
      "hidden": true,
      "name": "mingw",
      "toolchainFile": "$env{ACE}/mingw.cmake"
    },
    {
      "hidden": true,
      "name": "debug",
      "cacheVariables": {
        "BUILD_TESTING": true,
        "BUILD_SHARED_LIBS": true,
        "CMAKE_BUILD_TYPE": "Debug"
      }
    },
    {
      "hidden": true,
      "name": "release",
      "cacheVariables": {
        "BUILD_SHARED_LIBS": false,
        "CMAKE_BUILD_TYPE": "Release"
      }
    },
    {
      "hidden": true,
      "name": "relwithdebinfo",
      "cacheVariables": {
        "BUILD_SHARED_LIBS": true,
        "CMAKE_BUILD_TYPE": "RelWithDebInfo"
      }
    },
    {
      "hidden": true,
      "name": "coverage",
      "cacheVariables": {
        "BUILD_TESTING": true,
        "BUILD_SHARED_LIBS": true,
        "CMAKE_BUILD_TYPE": "Coverage"
      }
    },
    {
      "hidden": true,
      "name": "profile",
      "cacheVariables": {
        "BUILD_TESTING": true,
        "BUILD_SHARED_LIBS": true,
        "CMAKE_BUILD_TYPE": "Profile"
      }
    },
    {
      "name": "linux-debug",
      "displayName": "Linux: Debug",
      "inherits": [ "default", "linux", "debug" ]
    },
    {
      "name": "linux-release",
      "displayName": "Linux: Release",
      "inherits": [ "default", "linux", "release" ]
    },
    {
      "name": "linux-relwithdebinfo",
      "displayName": "Linux: RelWithDebInfo",
      "inherits": [ "default", "linux", "relwithdebinfo" ]
    },
    {
      "name": "linux-coverage",
      "displayName": "Linux: Coverage",
      "inherits": [ "default", "linux", "coverage" ]
    },
    {
      "name": "linux-profile",
      "displayName": "Linux: Profile",
      "inherits": [ "default", "linux", "profile" ]
    },
    {
      "name": "mingw-debug",
      "displayName": "MinGW: Debug",
      "inherits": [ "default", "mingw", "debug" ]
    },
    {
      "name": "mingw-release",
      "displayName": "MinGW: Release",
      "inherits": [ "default", "mingw", "release" ]
    },
    {
      "name": "mingw-relwithdebinfo",
      "displayName": "MinGW: RelWithDebInfo",
      "inherits": [ "default", "mingw", "relwithdebinfo" ]
    },
    {
      "name": "mingw-coverage",
      "displayName": "MinGW: Coverage",
      "inherits": [ "default", "mingw", "coverage" ]
    },
    {
      "name": "mingw-profile",
      "displayName": "MinGW: Profile",
      "inherits": [ "default", "mingw", "profile" ]
    }
  ],
  "buildPresets": [
    {
      "name": "linux-debug",
      "displayName": "Linux: Debug",
      "configurePreset": "linux-debug"
    },
    {
      "name": "linux-release",
      "displayName": "Linux: Release",
      "configurePreset": "linux-release"
    },
    {
      "name": "linux-relwithdebinfo",
      "displayName": "Linux: RelWithDebInfo",
